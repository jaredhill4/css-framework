# http://editorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = true

[{*.css,*.scss}]
indent_size = 2

[*.html]
indent_size = 4

[{*.js,*.jsx,*.json}]
indent_size = 2

[*.toml]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[.*rc]
indent_size = 2
